<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.10.xsd">

    <changeSet id="001-insert-data-users" author="ylab">
        <insert tableName="users">
            <column name="users_id" valueNumeric="1"/>
            <column name="users_firstName" value="Ivan"/>
            <column name="users_lastName" value="Ivanov"/>
            <column name="users_password" value="123"/>
        </insert>
    </changeSet>

    <changeSet id="002-insert-data-wallets" author="ylab">
        <insert tableName="wallets">
            <column name="wallets_id" valueNumeric="1"/>
            <column name="wallets_name" value="Ivan"/>
            <column name="wallets_balance" value="Ivanov"/>
        </insert>
    </changeSet>

    <changeSet id="003-insert-data-users_wallets" author="ylab">
        <insert tableName="users_wallets">
            <column name="users_wallets_id" valueNumeric="1"/>
            <column name="user_id" value="1"/>
            <column name="wallet_id" value="1"/>
        </insert>
    </changeSet>

</databaseChangeLog>